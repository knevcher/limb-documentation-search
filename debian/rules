#!/usr/bin/make -f
# -*- makefile -*-

LOCAL_REPO=tmp

build:
	sbt clean assembly

build-arch:
	build

build-indep:
	build

binary:
	dh_install
	dh_auto_install
	dh_installdirs
	dh_installdocs
	dh_installchangelogs
	dh_installmodules
	dh_installlogcheck
	dh_installlogrotate
	dh_installcron
	dh_installdebconf
	dh_installdeb
	dh_installinit
	dh_compress
	dh_fixperms
	dh_gencontrol
	dh_md5sums
	dh_builddeb
	dh_lintian

binary-arch:
	binary

binary-indep:
	binary

clean:
	dh_clean
